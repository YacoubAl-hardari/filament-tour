{
    "name": "yacoubalhaidari/filament-tour",
    "description": "An interactive guided tour for Filament Admin Panel using Shepherd.js",
    "keywords": [
        "yacoubalhaidari",
        "laravel",
        "filament",
        "filament-tour",
        "shepherd",
        "tour",
        "guide",
        "tutorial"
    ],
    "homepage": "https://github.com/yacoubalhaidari/filament-tour",
    "license": "MIT",
    "authors": [
        {
            "name": "Yacoub Alhaidari",
            "email": "your-email@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "YacoubAlhaidari\\FilamentTour\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YacoubAlhaidari\\FilamentTour\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YacoubAlhaidari\\FilamentTour\\FilamentTourServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

